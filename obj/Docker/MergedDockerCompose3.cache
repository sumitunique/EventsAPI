      dockerfile: EventsCRUD\Dockerfile -p
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: EventsCRUD
      target: base
    container_name: EventsCRUD
    entrypoint:
    - C:\remote_debugger\x64\msvsmon.exe
